{
    "name": "msgphp/organization",
    "type": "library",
    "license": "MIT",
    "description": "A domain layer providing basic organization management",
    "keywords": ["organization", "membership"],
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MsgPhp\\Organization\\": ""
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "msgphp/domain": "~0.6"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "symfony/phpunit-bridge": "^3.4|^4.0",
        "symfony/var-dumper": "^3.4|^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
